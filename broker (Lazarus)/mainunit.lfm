  object qDevLog: TSQLQuery
    FieldDefs = <    
      item
        Name = 'Device No'
        DataType = ftString
        Precision = -1
        Size = 5
      end    
      item
        Name = 'IP Address'
        DataType = ftString
        Precision = -1
        Size = 50
      end    
      item
        Name = 'User'
        DataType = ftString
        Precision = -1
        Size = 99
      end    
      item
        Name = 'Permissions'
        DataType = ftString
        Precision = -1
        Size = 64
      end    
      item
        Name = 'Last Activity'
        DataType = ftString
        Precision = -1
        Size = 13
      end>
    Database = ConfigurationForm.db
    Transaction = ConfigurationForm.Transaction
    SQL.Strings = (
      'SELECT'
      '  DATE_FORMAT(LogTime,''%d-%b %H:%i:%s'') AS "Date/Time",'
      '  UID AS "Device",'
      '  IPAddress AS "IP Address",'
      '  COALESCE(Name,"-") AS "User",'
      '  format(RunTimeMs,0) AS "Run-Time (ms)",'
      '  RxData AS "Data Received",'
      '  TxData AS "Data Sent"'
      'FROM mobiledevicelog'
      '  LEFT JOIN account ON AssignedUser=AccountID'
      'ORDER BY LogTime DESC'
      'LIMIT 500'
    )
    Params = <>
    left = 576
    top = 128
  end
