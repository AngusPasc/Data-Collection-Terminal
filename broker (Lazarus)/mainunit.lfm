  object CreateTables: TSQLQuery
    FieldDefs = <    
      item
        Name = 'Device No'
        DataType = ftString
        Precision = -1
        Size = 5
      end    
      item
        Name = 'IP Address'
        DataType = ftString
        Precision = -1
        Size = 50
      end    
      item
        Name = 'User'
        DataType = ftString
        Precision = -1
        Size = 99
      end    
      item
        Name = 'Permissions'
        DataType = ftString
        Precision = -1
        Size = 64
      end    
      item
        Name = 'Last Activity'
        DataType = ftString
        Precision = -1
        Size = 13
      end>
    Database = ConfigurationForm.db
    Transaction = ConfigurationForm.Transaction
    SQL.Strings = (
      'CREATE TABLE `mobiledevicelog`'
      '('
      '`ID` INT(11) NOT NULL AUTO_INCREMENT,'
      '`UID` VARCHAR(50) NOT NULL DEFAULT ''0'','
      '`IPAddress` VARCHAR(50) NOT NULL DEFAULT ''0'','
      '`RxData` VARCHAR(50) NOT NULL DEFAULT ''0'','
      '`TxData` VARCHAR(50) NOT NULL DEFAULT ''0'','
      '`RunTimeMs` INT(10) UNSIGNED NOT NULL DEFAULT ''0'','
      '`AssignedUser` INT(11) NULL DEFAULT NULL,'
      'PRIMARY KEY (`ID`)'
      ') ENGINE=InnoDB;'
      ''
      'CREATE TABLE IF NOT EXISTS `mobilequeries` ('
      '  `QID` int(10) unsigned NOT NULL AUTO_INCREMENT,'
      '  `QueryType` char(3) NOT NULL DEFAULT ''GET'','
      '  `QueryActive` tinyint(3) unsigned NOT NULL DEFAULT ''0'','
      '  `QueryDesc` varchar(99) DEFAULT NULL,'
      '  `FunctionCode` tinyint(3) unsigned DEFAULT NULL,'
      '  `QueryCode` text,'
      '  `InputFieldCount` tinyint(3) unsigned NOT NULL DEFAULT ''0'','
      '  `OutputFieldCount` tinyint(3) unsigned NOT NULL DEFAULT ''0'','
      '  `ReportFormat` text,'
      '  PRIMARY KEY (`QID`),'
      '  UNIQUE KEY `FunctionCode` (`FunctionCode`)'
      ') ENGINE=InnoDB;'
      ''
      'INSERT INTO `mobilequeries` (`QueryActive`, `QueryDesc`, `FunctionCode`, `QueryCode`, `InputFieldCount`, `OutputFieldCount`, `ReportFormat`)'
      'VALUES'
      '(1, ''Parameter1 is the jobid and returns all the assetids on this job in order'', 17, ''SELECT \r\n  AssetID AS "OutputField1"\r\nFROM movements\r\n  LEFT JOIN assets ON COALESCE(AssetToRef,AssetFromRef)=assetID \r\nWHERE JobRef="InputField1" \r\nORDER BY AssetName'', 0, 0, ''''),'
      '(1, ''Mobile system passes the JobID and it will return the job status, statusdesc and job title'', 16, ''SELECT \r\n  JobStatus AS "OutputField1",\r\n  name AS "OutputField2",\r\n  JobTitle AS "OutputField3"\r\nFROM b3marlborough.jobs \r\n  LEFT JOIN b3marlborough.lookupjobstatus ON JobStatus=ID\r\nWHERE JobID="InputField1"'', 1, 3, '''');'
      ''
      ''
    )
    Params = <>
    left = 848
    top = 128
  end
